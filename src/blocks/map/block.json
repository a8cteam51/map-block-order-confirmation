{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wc-order-map/map",
  "version": "0.1.0",
  "title": "Order Confirmation Map",
  "category": "woocommerce",
  "icon": "location-alt",
  "description": "Displays shipping address map on WooCommerce order confirmation pages",
  "supports": {
    "html": false,
    "align": true,
    "spacing": {
      "margin": true,
      "padding": true
    }
  },
  "attributes": {
    "zoom": {
      "type": "number",
      "default": 10
    },
    "height": {
      "type": "number",
      "default": 300
    }
  },
  "textdomain": "map-block-order-confirmation",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
} 